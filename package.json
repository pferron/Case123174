    "source-map": "^0.6.1"
  },
  "publishConfig": {
    "registry": "http://nexus.core.cvent.org:8081/nexus/content/repositories/npm-local/"
  },
  "sonar": {
    "sources": [
      "src"
    ]
  },
  "sourceMaps": {
    "android": "build/android/index.android.map",
    "ios": "build/ios/index.ios.map"
  }
}